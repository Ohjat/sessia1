      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Demo_ex.page"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="UpdateSurse">

    <Grid>

        <StackPanel Orientation="Horizontal">

            <StackPanel Width="400" Height="450" VerticalAlignment="Center" HorizontalAlignment="Center">
                <StackPanel Height="225"></StackPanel>
